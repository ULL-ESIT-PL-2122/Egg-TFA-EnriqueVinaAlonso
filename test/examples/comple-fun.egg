do(
  def(sum,  # function
    ->(nums, 
      do(
         def(i, 0), # Creates a local variable i and sets to 0
         def(s, 0), # Creates local var s and sets to 0
         while(<(i, length(nums)),
           do(=(s, +(s, <-(nums, i))),
              =(i, +(i, 1))
           )
         ),
         s # the last expression is returned 
      )
   )
 ),
 sum(array(1, 2, 3, 4))
)